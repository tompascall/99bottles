{
  "name": "99bottles",
  "version": "1.0.0",
  "description": "TS port of 99 Bottles of OOP",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tompascall/99bottles.git"
  },
  "keywords": [
    "oop",
    "ts",
    "kata",
    "tdd",
    "99 bottles"
  ],
  "author": "tompascall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tompascall/99bottles/issues"
  },
  "homepage": "https://github.com/tompascall/99bottles#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/lodash.range": "^3.2.6",
    "lodash": "^4.17.15",
    "lodash.range": "^3.2.0"
  }
}
